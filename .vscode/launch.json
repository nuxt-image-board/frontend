{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "Nuxt: Chrome",
        "url": "http://***REMOVED***:3000",
        "sourceMaps": true,
        "webRoot": "${workspaceFolder}"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Nuxt: Start",
        "console": "integratedTerminal",
        "cwd": "${workspaceFolder}",
        "args": ["dev", "--hostname=***REMOVED***"],
        "sourceMaps": true,
        "osx": {
          "program": "${workspaceFolder}/node_modules/.bin/nuxt"
        },
        "linux": {
          "program": "${workspaceFolder}/node_modules/.bin/nuxt"
        },
        "windows": {
          "program": "${workspaceFolder}/node_modules/nuxt/bin/nuxt.js"
        }
      }
    ],
    "compounds": [
      {
        "name": "Nuxt: StartBoth",
        "configurations": ["Nuxt: Start", "Nuxt: Chrome"]
      }
    ]
  }